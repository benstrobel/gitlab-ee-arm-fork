#!/bin/sh
exec 2>&1
<%= render("mount_point_check.erb") %>
umask 077
exec chpst -P -e <%= @options[:env_dir] %> \
  -U <%= @options[:username] %>:<%= @options[:groupname] %> \
  -u <%= @options[:username] %>:<%= @options[:groupname] %> \
  /opt/gitlab/embedded/bin/redis_exporter <%= @options[:flags] %>
